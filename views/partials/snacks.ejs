            <img src="<%= i.url%>" alt="">
            
          </a>
          <!-- <p>11 MINS</p> -->
          
          <h5><%= i.name%></h5>
          <br>
          <br>
          <br>
          
                      <p class="price">â‚¹<%= i.price%></p>
          
                      <form style="margin-left: 30px;" method="post" action="/product/single/:id">
